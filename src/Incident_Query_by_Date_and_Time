# Query for Date of Incident by Year and Time of Day
# Developed: July 3, 2015


# By Year

select INCIDENTDATE, Location where INCIDENTDATE  LIKE '%2008%';
from CRIME_2005_08182013


# By Year, with Advanced Filter (Expression), Column Filter

"INCIDENTDATE" LIKE '%2008-%'


# Time of Day

"INCIDENTTIME" LIKE '%0010%'
